    and sc.taxation_type_id in (select id from stepler_taxationtype where name in (&apos;УСН доходы 6%&apos;, &apos;Патент&apos;,&apos;УСН&apos;,&apos;Единый налог на временный доход (ЕНВД)&apos;)) 
