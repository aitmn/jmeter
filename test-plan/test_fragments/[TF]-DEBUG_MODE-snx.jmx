  &quot;jsonrpc&quot;: &quot;2.0&quot;,&#xd;
  &quot;id&quot;: &quot;${__UUID()}&quot;,&#xd;
  &quot;method&quot;: &quot;user_info_get&quot;,&#xd;
  &quot;params&quot;: {&#xd;
    &quot;external_key&quot;: &quot;${crypto_code_1}&quot;&#xd;
  }&#xd;
