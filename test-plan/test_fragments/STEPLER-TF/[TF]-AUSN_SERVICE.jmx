              <stringProp name="query">	          
select t1.customer_code
from stepler_customer t1

inner join

(select *
from stepler_customer
where id in (
select distinct customer_id
from stepler_serviceconnection
where status=&apos;CONNECTED&apos;
and service_id = (select id from stepler_service ss where name =&apos;FREE_TAX_COUNT&apos;)
)) t2
on t1.id = t2.id

where t1.id in (
select distinct customer_id
from stepler_serviceconnection
where status = &apos;DISCONNECTED&apos;
and service_id = (select id from stepler_service ss where name =&apos;TAX_COUNT&apos;)
)
order by random()
limit 1;</stringProp>
